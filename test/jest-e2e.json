{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {
      "allowJs": true
    }]
  },
  "setupFiles": ["dotenv/config"],
  "setupFilesAfterEnv": ["<rootDir>/jest-e2e.setup.js"],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1"
  }
}
