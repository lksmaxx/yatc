{
    "type": "postgres",
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "username": "${DB_USER}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_NAME}",
    "entities": [
        "dist/**/*.entity{.ts,.js}"
    ],
    "migrations": [
        "dist/migrations/*{.ts,.js}"
    ],
    "synchronize": false
}